<%= form_with model: @test, local: true do |form| %>
  <div class="form-group">
    <%= form.label :title %>
    <%= form.text_field :title, id: :test_title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :level %>
    <%= form.number_field :level, id: :test_level, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, promt: true %>
  </div>
  <%= form.submit class: 'btn btn-primary' %>
<% end %>