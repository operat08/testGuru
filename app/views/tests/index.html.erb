<div class="container m-4">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Quantity questions</th>
      </tr>
    </thead>
    <tbody>
      <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= question_quantity test %>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>