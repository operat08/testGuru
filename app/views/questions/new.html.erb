<h1><%= question_header :create, @test %></h1>
<div>
  <%= form_with model: [@test, @question], local: true do |form| %>
    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title, id: :question_title %>
    </p>
    <p>
      <%= form.submit 'Create' %>
    </p>
  <% end %>
</div>
<%= link_to "Back", test_questions_path(@test) %>
